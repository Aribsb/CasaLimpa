<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="casalimpa.projeto.com.casalimpa">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ico_casalimpa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ico_casalimpa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".MeuPerfilActivity" />
        <activity android:name=".Dashboard"></activity>
    </application>

</manifest>