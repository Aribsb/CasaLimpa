<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="casalimpa.projeto.com.casalimpa">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ico_casalimpa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ico_casalimpa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity"
                  android:label="Cadastro"
            />
        <activity android:name=".MeuPerfilActivity"
                  android:label="@string/meu_perfil"
            />
        <activity android:name=".Dashboard"
                  android:label="@string/title_dashboard"
            />
        <activity
            android:name=".MenuOpcao"
            android:label="@string/app_name"></activity>
    </application>

</manifest>